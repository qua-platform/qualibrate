{
  "name": "qualibrate-frontend-ui",
  "version": "1.0.0",
  "description": "Web Interface for QUAlibrate",
  "author": "QM",
  "license": "MIT",
  "private": true,
  "browserslist": "> 1%, last 2 versions, not dead",
  "homepage": ".",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js",
    "start": "webpack serve --config webpack.dev.js --open",
    "start:local:dev": "env-cmd -f .env webpack serve --config webpack.dev.js --open",
    "start:dev": "cross-env API_URL=https://dev.qualibrate-lab.io/ webpack serve --config webpack.dev.js --open",
    "clean": "rimraf ./dist",
    "start:testServer": "node scripts/testServer.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --ext ts,tsx src",
    "lint:style": "stylelint --fix -f verbose src/**/*.scss",
    "lint:fix": "npm run lint:code -- --fix && npm run lint:style -- --fix",
    "start:local": "cross-env API_URL=http://localhost/ npm run start",
    "prepare": "cd .. && husky frontend/.husky",
    "format": "prettier --write src public package.json",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest --coverage",
    "test:contexts": "vitest run src/contexts",
    "test:components": "vitest run src/modules",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:e2e": "cd tests && npm run test",
    "test:e2e:headed": "cd tests && npx playwright test --headed",
    "test:e2e:debug": "cd tests && npx playwright test --headed --debug",
    "test:e2e:ui": "cd tests && npx playwright test --ui"
  },
  "pre-commit": [
    "format"
  ],
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.14",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cytoscape": "^3.21.7",
    "@types/cytoscape-klay": "^3.1.4",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/ui": "^3.2.4",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-css-modules": "2.12.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "^4.19.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "jsonpath": "^1.1.1",
    "msw": "^2.11.3",
    "prettier": "^3.2.5",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.6.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.3.1",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@gitgraph/react": "^1.6.0",
    "@react-hook/switch": "1.3.3",
    "@textea/json-viewer": "^3.4.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.10.0",
    "cytoscape": "^3.30.2",
    "cytoscape-klay": "^3.1.4",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "flexlayout-react": "0.7.15",
    "javascript-time-ago": "2.5.9",
    "jsx-runtime": "^1.2.0",
    "mini-css-extract-plugin": "2.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.22.3",
    "react-toastify": "10.0.5",
    "refractor": "4.8.1",
    "stream": "0.0.2",
    "ts-loader": "^9.5.1",
    "yui-lint": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
