{
  "name": "qualibrate-frontend-ui",
  "version": "1.0.0",
  "description": "Web Interface for QUAlibrate",
  "author": "QM",
  "license": "MIT",
  "private": true,
  "browserslist": "> 1%, last 2 versions, not dead",
  "homepage": ".",
  "scripts": {
    "build": "webpack",
    "start": "webpack serve --open",
    "start:local:dev": "env-cmd -f .env webpack serve --open",
    "start:dev": "cross-env API_URL=https://dev.qualibrate-lab.io/ webpack serve --open",
    "clean": "rimraf ./dist",
    "start:testServer": "node scripts/testServer.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --ext ts,tsx src",
    "lint:style": "stylelint --fix -f verbose src/**/*.scss",
    "lint:fix": "npm run lint:code -- --fix && npm run lint:style -- --fix",
    "start:local": "cross-env API_URL=http://localhost/ npm run start",
    "prepare": "cd .. && husky frontend/.husky",
    "format": "prettier --write src public package.json"
  },
  "pre-commit": [
    "format"
  ],
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.14",
    "@types/cytoscape": "^3.21.7",
    "@types/cytoscape-klay": "^3.1.4",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-css-modules": "2.12.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "express": "^4.19.2",
    "husky": "^9.1.7",
    "jsonpath": "^1.1.1",
    "prettier": "^3.2.5",
    "sass": "^1.89.0",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.6.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-prettier": "^5.0.0",
    "stylelint-scss": "^6.3.0",
    "typescript": "^5.4.2",
    "typescript-eslint": "^7.3.1",
    "webpack": "^5.91.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.1"
  },
  "dependencies": {
    "@gitgraph/react": "^1.6.0",
    "@react-hook/switch": "1.3.3",
    "@textea/json-viewer": "^3.4.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "^6.10.0",
    "cytoscape": "^3.30.2",
    "cytoscape-klay": "^3.1.4",
    "dotenv-webpack": "^8.0.1",
    "file-loader": "^6.2.0",
    "flexlayout-react": "0.7.15",
    "html-webpack-plugin": "^5.5.0",
    "javascript-time-ago": "2.5.9",
    "jsx-runtime": "^1.2.0",
    "mini-css-extract-plugin": "2.7.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.22.3",
    "react-toastify": "10.0.5",
    "refractor": "4.8.1",
    "stream": "0.0.2",
    "ts-loader": "^9.5.1",
    "yui-lint": "^0.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
